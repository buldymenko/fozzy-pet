{
    "name": "eugene/fozzy-pet",
    "require": {
        "symfony/console": "^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/dotenv": "^5.0"
    },
    "authors": [
        {
            "name": "buldymenko",
            "email": "buldimenko@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\Services\\": "src/Services",
            "App\\Entity\\": "src/Entity",
            "App\\Console\\Command\\": "src/Console/Commands",
            "App\\Repository\\": "src/Repository"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true
    }
}