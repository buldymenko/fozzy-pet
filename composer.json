{
    "name": "eugene/fozzy-pet",
    "require": {
        "symfony/console": "^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/dotenv": "^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "authors": [
        {
            "name": "buldymenko",
            "email": "buldimenko@gmail.com"
        }
    ],
    "require-dev": {
        "symfony/maker-bundle": "^1.15",
        "symfony/phpunit-bridge": "^5.0",
        "dama/doctrine-test-bundle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\Services\\": "src/Services",
            "App\\Entity\\": "src/Entity",
            "App\\Console\\Command\\": "src/Console/Commands",
            "App\\Repository\\": "src/Repository"
        },
        "classmap":["app/AppKernel.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\Console\\Command\\": "tests/Console/Command/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
